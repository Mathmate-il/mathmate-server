{
  "name": "mathmate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "cross:sleep": "( sleep 4 || timeout /t 4)",
    "test:env": "dotenv --e .env.test -- ",
    "clean:dist": "rimraf dist",
    "db:dev:rm": "docker-compose rm db -s -f -v",
    "db:dev:stop": "docker-compose stop db",
    "db:dev:up": "docker-compose up db -d",
    "db:dev:restart": "npm run db:dev:up && npm run cross:sleep && prisma migrate dev",
    "db:test:rm": "docker-compose rm db-test -s -f -v",
    "db:test:up": "npm run test:env --  docker-compose up db-test -d",
    "db:test:prisma": "npm run test:env --  prisma migrate dev",
    "db:test:restart": "npm run test:env --  npm run db:test:rm && npm run db:test:up && npm run cross:sleep  && npm run db:test:prisma",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run db:dev:restart && npx prisma db seed && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx prisma migrate deploy && npx prisma db seed && node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv --e .env.test -- npm run db:dev:stop && npm run db:test:restart && dotenv --e .env.test -- npx prisma db seed &&npm run cross:sleep && npm run test:env --  jest --config ./test/jest-e2e.json"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed/seed.ts"
  },
  "dependencies": {
    "@nestjs/class-validator": "^0.13.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^3.1.0",
    "@origranot/ts-logger": "^1.7.0",
    "@prisma/client": "^4.8.0",
    "@types/chalk": "^2.2.0",
    "@types/reflect-metadata": "^0.1.0",
    "body-parser": "^1.20.1",
    "chalk": "^5.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "google-auth-library": "^8.7.0",
    "hbs": "^4.2.0",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-client": "^3.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.3.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/morgan": "^1.9.4",
    "@types/node": "^16.0.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "prisma": "^4.8.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@repositories/(.*)$": "<rootDir>/../src/repositories/entities/$1",
      "^@services/(.*)$": "<rootDir>/../src/services/$1"
    }
  }
}
